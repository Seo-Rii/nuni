{
  "name": "nunui",
  "description": "Svelte UI Component Library",
  "version": "0.0.110",
  "scripts": {
    "dev": "vite dev",
    "build": "svelte-kit sync && svelte-package",
    "build-page": "vite build",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "package": "svelte-kit sync && svelte-package",
    "publish": "npm run package && cd package && npm publish",
    "page": "npm run build-page && node gh-pages.js"
  },
  "devDependencies": {
    "@sveltejs/adapter-static": "^2.0.3",
    "@sveltejs/kit": "^1.27.5",
    "@sveltejs/package": "^1.0.2",
    "@types/codemirror": "^5.60.13",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "codemirror-svelte": "^2.0.0",
    "eslint": "8.53.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "gh-pages": "^6.0.0",
    "prettier": "^3.1.0",
    "prettier-plugin-svelte": "^3.1.0",
    "sass": "^1.69.5",
    "svelte": "^4.2.3",
    "svelte-check": "^3.6.0",
    "svelte-preprocess": "^5.1.0",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2",
    "vite": "^4.5.0"
  },
  "type": "module",
  "dependencies": {
    "codemirror": "^6.0.1",
    "colorjs.io": "^0.4.5",
    "date-fns": "^2.30.0",
    "date-picker-svelte": "^2.9.0",
    "svelte-portal": "^2.2.0"
  }
}
